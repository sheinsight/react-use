{
  "name": "shined-react-use-website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docs:locale": "esno ./scripts/copy-locale-markdown.mts",
    "docs:update": "pnpm run docs:locale && esno ./scripts/update-hooks-data.mts",
    "dev": "pnpm run docs:update && docusaurus start",
    "start": "pnpm run dev",
    "build": "pnpm run update:data && docusaurus build",
    "serve": "pnpm run update:data && docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@docusaurus/core": "3.4.0",
    "@docusaurus/preset-classic": "3.4.0",
    "@formkit/auto-animate": "^0.8.2",
    "@mdx-js/react": "^3.0.1",
    "change-case": "^5.4.4",
    "clsx": "^2.1.1",
    "prism-react-renderer": "^2.3.1",
    "raw-loader": "^4.0.2",
    "react-hot-toast": "^2.4.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.4.0",
    "@docusaurus/tsconfig": "3.4.0",
    "@docusaurus/types": "3.4.0",
    "@unocss/webpack": "^0.61.0",
    "esno": "^4.7.0",
    "globby": "^14.0.1",
    "gray-matter": "^4.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "~5.2.2",
    "unocss": "^0.61.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
